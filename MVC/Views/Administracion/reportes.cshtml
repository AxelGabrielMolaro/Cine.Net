
@{
    ViewBag.Title = "reportes";
    Layout = "~/Views/Shared/_AdministradorBase.cshtml";
}

<div class="text-center"><h2>Reportes</h2></div>

<div class="col-md-6">
    <div class="text-center"><h3>Buscas reservas</h3></div>
    <div>
        <form class="form">
            <div class="form-group">
                <label for="" class="control-label">Fecha desde</label>
                <div>
                    <input type="date" class="form-control" id="fechaInicio" />
                </div>
                
            </div>
            <div class="form-group">
                <label for="" class="control-label">Fecha hasta</label>
                <div>
                    <input type="date" class="form-control"  id="fechaHasta"/>
                </div>
            </div>
            <input value="Buscar" class="btn btn-block btn-success"/>

        </form>
    </div>
</div>
 <div class="col-md-6">
    <div class="text-center"><h3>Reportes de reservas</h3></div>
    <table class="table ">

        <thead>

            <tr class="success">
                <th>Sede</th>
                <th>Versión</th>
                <th>Nombre de la película</th>
                <th>Precio</th>

            </tr>
        </thead>
        <tbody>

            <tr class="warning">
                <td class="col-md-2">SHOPPING SJ</td>
                <td class="col-md-2">Español</td>
                <td class="col-md-2">Rocky3</td>
                <td class="col-md-2">$20</td>
            </tr>

        </tbody>

        
    </table>
</div>
